<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="FilesUploadedPath" value="/App_Data/FileUploaded/"/>
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <customErrors mode="On"></customErrors>
    
  </system.web>
  <system.webServer>
    <defaultDocument>
      <files>
        <clear />
        <add value="Home.aspx"/>
      </files>
    </defaultDocument>
  </system.webServer>
</configuration>
